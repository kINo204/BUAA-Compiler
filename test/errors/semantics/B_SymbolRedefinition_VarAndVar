void putString(char s[], int length){
    int i = 0;
    char i;
    for(;i < length; i = i + 1){
        int i;
        printf("%c", s[i]);
    }
    printf("\n");
}

int strlen(char s[]){
    int i = 0;
    for(; s[i] != '\0'; i = i + 1) ;
    return i;
}

char charAt(char s[], int index){
    return s[index];
}

int main(){
    day = getDay();
